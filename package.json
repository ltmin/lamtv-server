{
  "name": "lamtv",
  "version": "1.0.0",
  "description": "lamtv app",
  "scripts": {
    "start": "nodemon --max-old-space-size=4096 ./src/main.js --config nodemon.json",
    "clean": "rimraf ./dist",
    "build": "npm run clean && babel src -d dist --copy-files",
    "test": "jest --config jest.config.json",
    "test:unit": "jest --config jest.config.json",
    "test:coverage": "jest --coverage --config jest.config.json",
    "db:re-index": "babel-node ./src/db-cli.js re-index",
    "db:seed": "babel-node ./src/db-cli.js seed",
    "db:migrate": "babel-node ./src/db-cli.js migrate",
    "docker:image": "IMAGE_NAME=wedding ./scripts/docker-image.sh"
  },
  "author": "ltmin",
  "dependencies": {
    "body-parser": "^1.19.0",
    "common-node": "git+ssh://git@github.com:ltmin/common-node.git",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "helmet": "^4.1.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "keymirror": "^0.1.1",
    "lamtv-wedding": "git+ssh://git@github.com/ltmin/lamtv-wedding.git",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "ramda": "^0.27.1",
    "url-pattern": "^1.0.3",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.10.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^26.3.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  }
}
